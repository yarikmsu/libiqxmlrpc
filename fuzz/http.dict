"Content-Encoding:"
"Content-Encoding: gzip"
"Content-Encoding: identity"

# Unicode homograph in headers
"\xE2\x80\x8B"

# Host header attacks (SSRF, cache poisoning)
"Host: evil.com"
"Host: 169.254.169.254"
"Host: 127.0.0.1"
"Host: 0.0.0.0"
"Host: [::ffff:127.0.0.1]"
"X-Forwarded-Host:"
"X-Original-URL:"
"X-Rewrite-URL:"
"X-Forwarded-For:"
"X-Real-IP:"
"X-Originating-IP:"
"X-Client-IP:"

# Cache poisoning headers
"X-Forwarded-Proto:"
"X-Forwarded-Scheme:"
"X-HTTP-Method-Override:"
"X-Method-Override:"
"X-Original-Method:"
"Pragma: no-cache"
"Cache-Control: no-cache, no-store"
"Vary:"
"Age:"

# Additional smuggling patterns
"Transfer-Encoding: chunked"
"Transfer-Encoding:\x09chunked"
"Transfer-Encoding : chunked"
"Transfer-Encoding: xchunked"
"Transfer-Encoding: chunked\x0d\x0aTransfer-Encoding: x"
" Transfer-Encoding: chunked"
"X-Transfer-Encoding: chunked"
"Transfer-encoding: chunked"

# Chunked body patterns
"0\x0d\x0a\x0d\x0a"
"1\x0d\x0aX\x0d\x0a0\x0d\x0a\x0d\x0a"
"FFFFFFFF\x0d\x0a"

# WebSocket upgrade (protocol confusion)
"Upgrade: websocket"
"Upgrade: h2c"
"Connection: Upgrade"
"Sec-WebSocket-Key:"
"Sec-WebSocket-Version:"

# HTTP/2 pseudo-headers (downgrade attacks)
":method"
":path"
":authority"
":scheme"

# Response splitting
"\x0d\x0aSet-Cookie: session=evil"
"\x0d\x0aLocation: http://evil.com"

# Multipart boundary attacks
"multipart/form-data"
"boundary=----"
"--boundary--"

# Unicode normalization in headers
# Zero-width characters (header value smuggling)
# "\xE2\x80\x8B" - already defined above (line 215)
"\xE2\x80\x8C"
"\xE2\x80\x8D"
"\xEF\xBB\xBF"

# Bidirectional text (visual spoofing)
"\xE2\x80\xAE"
"\xE2\x80\xAD"

# Homoglyphs for header names
"\xD0\xB0"
"\xCE\xB1"

# Full-width colon and space (parser confusion)
"\xEF\xBC\x9A"
"\xE3\x80\x80"
