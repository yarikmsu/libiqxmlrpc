"\xEF\xBB\xBF"

# Numeric character references
"&#65;"
"&#x41;"
"&#0;"
"&#x0;"
"&#10;"
"&#13;"

# Method call/response
"<methodCall>"
"</methodCall>"
"<methodName>"
"</methodName>"
"<methodResponse>"
"</methodResponse>"

# Parameters
"<params>"
"</params>"
"<param>"
"</param>"

# Values
"<value>"
"</value>"

# Scalar types
"<i4>"
"</i4>"
"<i8>"
"</i8>"
"<int>"
"</int>"
"<boolean>"
"</boolean>"
"<string>"
"</string>"
"<double>"
"</double>"
"<dateTime.iso8601>"
"</dateTime.iso8601>"
"<base64>"
"</base64>"
"<nil/>"
"<nil>"
"</nil>"
