# XXE/DTD tokens (for security testing)
"<!DOCTYPE"
"<!ENTITY"
"SYSTEM"
"PUBLIC"
"file://"
"http://"
"<!ATTLIST"
"<!ELEMENT"
"]>"

# UTF-8 edge cases (encoding vulnerabilities)
"\xC0\xAF"
"\xE0\x80\xAF"
"\xF0\x80\x80\xAF"
"\xED\xA0\x80"
"\xED\xBF\xBF"
"\xF4\x90\x80\x80"
"\xFF\xFE"

# XML bomb / billion laughs patterns (entity expansion)
"&lol;"
"&lol1;"
"&lol2;"
"<!ENTITY lol \"lol\">"
"<!ENTITY lol1 \"&lol;&lol;&lol;&lol;&lol;\">"
"<!ENTITY lol2 \"&lol1;&lol1;&lol1;&lol1;&lol1;\">"

# Parameter entities (DTD injection)
"%entity;"
"<!ENTITY % dtd"
"%dtd;"
"<!ENTITY % file SYSTEM"

# Processing instructions
"<?xml-stylesheet"
"type=\"text/xsl\""
"href=\""
# "?>" - already defined above (line 10)

# External DTD URL schemes
"gopher://"
"ftp://"
"jar:"
"netdoc:"

# Null byte in content
"\x00"

# XInclude patterns (XML inclusion attacks)
"<xi:include"
"xmlns:xi=\"http://www.w3.org/2001/XInclude\""
# "href=\"" - already defined above (line 178)
"parse=\"xml\""
"parse=\"text\""
"xpointer=\""
"</xi:include>"
"<xi:fallback>"
"</xi:fallback>"

# Encoding declarations (parser confusion attacks)
"encoding=\"UTF-16\""
"encoding=\"UTF-32\""
"encoding=\"ISO-8859-1\""
"encoding=\"US-ASCII\""
"encoding=\"windows-1252\""
"standalone=\"yes\""
"standalone=\"no\""

# UTF-16 BOM patterns
# "\xFF\xFE" - already defined above (line 159)
"\xFE\xFF"

# UTF-32 BOM patterns
"\x00\x00\xFE\xFF"
"\xFF\xFE\x00\x00"

# XML 1.1 declaration (different parsing rules)
"version=\"1.1\""

# Namespace confusion
"xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\""
"xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\""
"xsi:type=\""

# Recursive entity definitions (DoS)
"<!ENTITY a \"&b;&b;&b;&b;&b;\">"
"<!ENTITY b \"&c;&c;&c;&c;&c;\">"
"<!ENTITY c \"&d;&d;&d;&d;&d;\">"

# External entity with different protocols
"php://filter"
"expect://"
"data:"
"file:///etc/passwd"
"file:///c:/windows/win.ini"

